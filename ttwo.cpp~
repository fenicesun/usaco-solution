#include <iostream>
#include <cstdio>
#include <string>
#include <cstring>
#include <vector>
#include <map>
#include <set>
#include <algorithm>
#include <cstdlib>
#include <list>
#include <cmath>

using namespace std;

#define MAXN 110

//gobal variable====
int forest[MAXN][MAXN];
int state[MAXN][MAXN][5];
int Fx, Fy, Cx, Cy;
int result;
int fstep, cstep;
//==================


//function==========
void print(){
	for(int i = 1; i <= 10; i++){
		for(int j = 1; j <= 10; j++){
			cout<<forest[i][j]<<" ";
		}
		cout<<endl;
	}
}

//1 represent obstacle
void Input(){
	char ch;
	for(int i = 1; i <= 10; i++){
		for(int j = 1; j <= 10; j++){
			cin>>ch;
			if(ch == '.')
				forest[i][j] = 0;
			if(ch == '*')
				forest[i][j] = 1;
			if(ch == 'F'){
				forest[i][j] = 0;
				Fx = i, Fy = j; 
			}
			if(ch == 'C'){
				forest[i][j] = 0;
				Cx = i, Cy = j;
			}
		}
	}
}

void solve(){
	//2d BFS
	fstep = 0, cstep = 0;
	
}

//==================

int main(){
	freopen("ttwo.in","r",stdin);
	freopen("ttwo.out","w",stdout);
	Input();
	solve();
	print();
	return 0;
}
